<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PackageId>OpenFeature.Contrib.Providers.Flipt</PackageId>
		<VersionNumber>0.0.1</VersionNumber><!--x-release-please-version -->
		<VersionPrefix>$(VersionNumber)</VersionPrefix>
		<AssemblyVersion>$(VersionNumber)</AssemblyVersion>
		<FileVersion>$(VersionNumber)</FileVersion>
		<Description>Flipt provider for .NET</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Authors>Rogov Dmitry</Authors>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Flipt.gRPC" Version="0.1.3" />
